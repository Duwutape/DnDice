<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="nameLabel" prefHeight="25.0" prefWidth="30.0" text="d2:">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin>
      </Label>
      <Button fx:id="subButton" mnemonicParsing="false" prefWidth="25.0" text="-" />
      <TextField fx:id="countField" alignment="CENTER" editable="false" prefWidth="50.0" text="0" />
      <Button fx:id="addButton" mnemonicParsing="false" text="+" />
      <Button fx:id="resetButton" mnemonicParsing="false" text="reset">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
